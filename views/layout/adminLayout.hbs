<script>
    $(document).ready(function () {
        $('.delete').click(function () {
            const catID = $(this).attr('catId')
            axios.delete(`/admin/edit-category/${catID}`).then(e => {
                if (e.data.status == "failed") {
                    swal(e.data.message, {
                        icon: "success",
                    });
                } else {
                    location.reload()
                }

            })
        })
    })
</script>
