                                    {{!-- <td>{{this._id}}</td> --}}
                                    {{!-- <td>{{itemsCount this.products}}</td> --}}
                                    <td>{{this.userId.userName}}</td>
                                    <td><span class="currenc">â‚¹ </span>{{this.finalCost}}</td>
                                    <td>{{this.paymentMethod}}</td>
                                    <td>{{this.paymentStatus}}</td>
                                    <td>{{this.orderStatus}}</td>
                                    <td><a href="/admin/orderdetails/{{this._id}}"><i class="text-danger fa fa-eye d-flex justify-content-center" aria-hidden="true"></i></a></td>
                                    {{#if (eq orderStatus 'Placed')}}
                                    <td><a style="font-color: black;" onclick="packOrder('{{this._id}}')"><i style="color: black;" class="text-danger fa fa-share-square d-flex justify-content-center" data-toggle="tooltip" data-placement="top" title="Pack Order" aria-hidden="true"></i></a></td>
                                    {{else if (eq orderStatus "Packed")}}
                                    <td><a style="font-color: black;" onclick="shipOrder('{{this._id}}')"><i style="color: black;" class="text-danger fa fa-shopping-bag d-flex justify-content-center" data-toggle="tooltip" data-placement="top" title="Ship Order" aria-hidden="true"></i></a></td>
                                    {{else if (eq orderStatus "Shipped")}}
                                    <td><a style="font-color: black;" onclick="deliverOrder('{{this._id}}','{{this.userId._id}}')"><i style="color: black;" class="text-danger fa fa-truck d-flex justify-content-center" data-toggle="tooltip" data-placement="top" title="Deliver Order" aria-hidden="true"></i></a></td>
                                    {{else if (eq orderStatus "Delivered")}}
                                    <td><a style="font-color: black;"><i style="color: black;" class="text-danger fa fa-check-square d-flex justify-content-center" data-toggle="tooltip" data-placement="top" title="Order Delivered" aria-hidden="true"></i></a></td>
                                    {{!-- <td><a href=""><i class="text-danger fa fa-times d-flex justify-content-center" aria-hidden="true"></i></a></td> --}}
                                    {{/if}}
                                    {{#twoConditions paymentStatus "Confirmed" orderStatus "Delivered"}}
                                    <td>
                                        <button class="btn btn-sm btn-outline-light" disabled>Cancel</button>
                                    </td>
                                    {{else}}
                                    <div class="d-flex align-items-center justify-content-center"><td><button class="btn btn-sm btn-light" onclick="cancelOrder('{{this._id}}')">Cancel</button></td></div>
                                    {{/twoConditions}}
                                    
                                </tr>
